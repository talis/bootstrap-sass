{
  "name": "bootstrap-sass",
  "version": "3.3.6-talis.24",
  "description": "bootstrap-sass is a Sass-powered version of Bootstrap 3, ready to drop right into your Sass powered applications.",
  "main": "assets/javascripts/bootstrap.js",
  "style": "assets/stylesheets/_bootstrap.scss",
  "sass": "assets/stylesheets/_bootstrap.scss",
  "repository": {
    "type": "git",
    "url": "git://github.com/twbs/bootstrap-sass"
  },
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "eyeglass-module"
  ],
  "contributors": [
    "Thomas McDonald",
    "Tristan Harward",
    "Peter Gumeson",
    "Gleb Mazovetskiy"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twbs/bootstrap-sass/issues"
  },
  "devDependencies": {
    "ejs": "~2.4.2",
    "http-server": "^0.11.1",
    "mincer": "~1.4.0",
    "node-sass": "^3.13.1"
  },
  "eyeglass": {
    "exports": "eyeglass-exports.js",
    "needs": "^0.7.1"
  },
  "scripts": {
    "build-compressed": "./node_modules/node-sass/bin/node-sass app.scss dist/app.min.css --output-style compressed",
    "build-uncompressed": "./node_modules/node-sass/bin/node-sass app.scss dist/app.css",
    "build": "npm run build-compressed && npm run build-uncompressed",
    "serve": "http-server",
    "watch": "./node_modules/node-sass/bin/node-sass --watch app.scss dist/app.min.css --output-style compressed"
  }
}
