{
  "name": "bootstrap-sass",
  "version": "3.3.6-talis.32",
  "description": "bootstrap-sass is a Sass-powered version of Bootstrap 3, ready to drop right into your Sass powered applications.",
  "main": "assets/javascripts/bootstrap.js",
  "style": "assets/stylesheets/_bootstrap.scss",
  "sass": "assets/stylesheets/_bootstrap.scss",
  "repository": {
    "type": "git",
    "url": "git://github.com/twbs/bootstrap-sass"
  },
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "eyeglass-module"
  ],
  "contributors": [
    "Thomas McDonald",
    "Tristan Harward",
    "Peter Gumeson",
    "Gleb Mazovetskiy"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twbs/bootstrap-sass/issues"
  },
  "devDependencies": {
    "ejs": "~2.4.2",
    "es-dev-server": "^1.57.1",
    "mincer": "~1.4.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "standard-version": "^8.0.2"
  },
  "eyeglass": {
    "exports": "eyeglass-exports.js",
    "needs": "^0.7.1"
  },
  "scripts": {
    "build-compressed": "npx node-sass app.scss dist/app.min.css --output-style compressed",
    "build-uncompressed": "npx node-sass app.scss dist/app.css",
    "build": "npm run build-compressed && npm run build-uncompressed",
    "serve": "es-dev-server --watch --app-index index.html",
    "start": "run-p watch serve",
    "watch": "npx node-sass --watch app.scss dist/app.css",
    "prerelease": "npm run build",
    "release": "git add dist/ && standard-version --prerelease talis -a"
  }
}
