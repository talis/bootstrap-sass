{
  "name": "bootstrap-sass",
  "version": "3.7.11",
  "description": "bootstrap-sass is a Sass-powered version of Bootstrap 3, ready to drop right into your Sass powered applications.",
  "main": "assets/javascripts/bootstrap.js",
  "style": "assets/stylesheets/_bootstrap.scss",
  "sass": "assets/stylesheets/_bootstrap.scss",
  "files": [
    "assets",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/talis/bootstrap-sass"
  },
  "contributors": [
    "Thomas McDonald",
    "Tristan Harward",
    "Peter Gumeson",
    "Gleb Mazovetskiy"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talis/bootstrap-sass/issues"
  },
  "devDependencies": {
    "ejs": "~2.4.2",
    "es-dev-server": "^1.57.1",
    "mincer": "~1.4.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "standard-version": "^8.0.2"
  },
  "scripts": {
    "build:scripts": "",
    "build:styles:compressed": "npx node-sass app.scss dist/app.min.css --output-style compressed --source-map true",
    "build:styles:uncompressed": "npx node-sass app.scss dist/app.css --source-map true",
    "build": "npm run build:styles:compressed && npm run build:styles:uncompressed",
    "serve": "es-dev-server --watch --app-index index.html",
    "start": "run-p watch serve",
    "watch": "npx node-sass --watch app.scss dist/app.css --source-map true",
    "prerelease": "npm run build",
    "release": "git add dist/ && standard-version -a"
  }
}
